{
  "expo": {
    "name": "testApp01",
    "slug": "testApp01",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./testApp01/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./testApp01/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./testApp01/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./testApp01/assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}